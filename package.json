{
  "name": "react-cached-callback",
  "version": "1.0.1",
  "description": "Library for excluding function generations on React's render function call",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "node ./node_modules/ts-node/dist/bin ./node_modules/tape/bin/tape ./src/**.spec.ts",
    "build": "node ./node_modules/typescript/bin/tsc -p tsconfig.base.json",
    "prepublishOnly": "npm run test && npm run build",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "commit": "git-cz"
  },
  "keywords": [
    "react",
    "bind",
    "reselect",
    "cache",
    "arrow-function"
  ],
  "devDependencies": {
    "@types/node": "^8.0.46",
    "@types/tape": "^4.2.30",
    "commitizen": "^3.1.1",
    "conventional-changelog-cli": "^2.0.17",
    "cz-conventional-changelog": "^2.1.0",
    "nodemon": "^1.12.1",
    "tape": "^4.8.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/megazazik/react-cached-callback.git"
  },
  "homepage": "https://github.com/megazazik/react-cached-callback",
  "author": "Michael Lebedev",
  "email": "megazazik@rambler.ru",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
